/*c-chat-comp start*/
.c-chat-comp {
  .fontText {
    font-size: large;
  }

  .phase-active {
    border: solid 1px $grey-one;
    background: $default;
    color: $white;
  }

  #chat-scroll {
    height: 450px;
    overflow: hidden;
    outline: none;
  }

  .card.chat {
    height: 100%;
  }

  .card-body.chat-content.no-msg-wrapper {
    text-align: center;
    flex-wrap: wrap !important;
    display: flex !important;
    align-items: center;
    justify-content: center;
    line-height: normal;
    margin-top: 18%;
    height: auto !important;

    img.no-msg {
      max-width: 120px;
    }

    p {
      width: 100%;
    }
  }

  .chat {
    .chat-message {
      .fa-file-o {
        font-size: 16px;
        color: gray;
        cursor: pointer;
      }

      .fa-file-image-o {
        font-size: 16px;
        color: gray;
        cursor: pointer;
      }

      padding: 20px;

      textarea {
        width: 100%;
        border: none;
        padding: 10px 20px;
        font: 14px/22px Lato, Arial, sans-serif;
        margin-bottom: 10px;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        -ms-border-radius: 5px;
        border-radius: 5px;
        resize: none;
      }
    }

    .chat-header {
      padding: 20px;
      border-bottom: 1px solid $grey-one;
      border-radius: 0 0.55rem 0 0;

      img {
        float: left;
        -webkit-border-radius: 50%;
        -moz-border-radius: 50%;
        -ms-border-radius: 50%;
        border-radius: 50%;
        width: 35px;
        height: 35px;
      }

      .chat-about {
        float: left;
        padding-left: 10px;
      }

      .chat-with {
        font-weight: bold;
        font-size: 16px;
      }

      .chat-num-messages {
        color: #434651;
      }
    }

    .chat-history {
      padding: 20px;
      border-bottom: 2px solid $white;
      height: 450px;

      ul {
        padding: 0;

        li {
          list-style: none;
        }
      }

      .message-data {
        margin-bottom: 15px;

        .message-data-name {
          font-size: 13px;
          font-weight: 700;
        }
      }

      .message-data-time {
        color: #434651;
        padding-left: 6px;
      }

      .message {
        color: #444;
        padding: 18px 20px;
        line-height: 26px;
        font-size: 13px;
        -webkit-border-radius: 7px;
        -moz-border-radius: 7px;
        -ms-border-radius: 7px;
        border-radius: 7px;
        margin-bottom: 30px;
        width: 90%;
        position: relative;

        &:after {
          bottom: 100%;
          left: 7%;
          border: solid transparent;
          content: " ";
          height: 0;
          width: 0;
          position: absolute;
          pointer-events: none;
          border-bottom-color: $white;
          border-width: 10px;
          margin-left: -10px;
        }
      }

      .my-message {
        background: $backgroundColor2;

        &:after {
          bottom: 100%;
          left: 7%;
          border: solid transparent;
          content: " ";
          height: 0;
          width: 0;
          position: absolute;
          pointer-events: none;
          border-bottom-color: $backgroundColor2;
          border-width: 10px;
          margin-left: -10px;
        }
      }

      .other-message {
        background: $backgroundColor2;

        &:after {
          border-bottom-color: $backgroundColor2;
          left: 93%;
        }
      }
    }
  }

  .chat-app {
    height: 690px;

    .people-list {
      width: 280px;
      position: absolute;
      left: 0;
      top: 0;
      padding: 20px;
    }

    .chat {
      border-left: 1px solid $secondarybtn;
    }

    .list_btn {
      position: fixed;
      bottom: 20px;
      right: 20px;
      z-index: 9999;
      padding: 0;
      width: 40px;
      height: 40px;
      text-align: center;
      line-height: 40px;
      display: none;
      -webkit-box-shadow: 0 10px 25px 0 rgba(0, 0, 0, 0.3);
      -moz-box-shadow: 0 10px 25px 0 rgba(0, 0, 0, 0.3);
      -ms-box-shadow: 0 10px 25px 0 rgba(0, 0, 0, 0.3);
      box-shadow: 0 10px 25px 0 rgba(0, 0, 0, 0.3);
      -webkit-border-radius: 3px;
      -moz-border-radius: 3px;
      -ms-border-radius: 3px;
      border-radius: 3px;
    }
  }

  .people-list {
    transition: 0.5s;

    .chat-search {
      margin: 10px;
    }

    .chat-list {
      li {
        &:hover {
          background: $backgroundColor2;
          cursor: pointer;
        }

        .name {
          font-size: 15px;
          padding: 10px 15px;
          width: 100%;
        }
      }

      li.active {
        background: $backgroundColor2;
      }

      img {
        width: 40px;
        border-radius: 50%;
        height: 40px;
      }
    }

    img {
      float: left;
      border: 1px solid $white;
      box-shadow: 0px 1px 13px #d3cfcfc2;
      -webkit-border-radius: 50%;
      -moz-border-radius: 50%;
      -ms-border-radius: 50%;
      border-radius: 50%;
    }

    .status {
      color: #999;
      font-size: 13px;
    }

    .about {
      button.name {
        display: flex;
        align-items: center;

        img {
          margin: 0px 15px 0px 0px;
        }
      }
    }
  }

  .c-chat.people-list {
    .chat-list {
      li {
        list-style: none;
        -webkit-border-radius: 3px;
        -moz-border-radius: 3px;
        -ms-border-radius: 3px;
        border-radius: 3px;
      }
    }
  }

  .online {
    margin-right: 3px;
    font-size: 10px;
    color: $successgreen;
  }

  .offline {
    margin-right: 3px;
    font-size: 10px;
    color: #e38968;
  }

  .me {
    margin-right: 3px;
    font-size: 10px;
    color: #0498bd;
  }

  .clearfix {
    &:after {
      visibility: hidden;
      display: block;
      font-size: 0;
      content: " ";
      clear: both;
      height: 0;
    }
  }

  .slimScrollBar {
    z-index: 0 !important;
  }

  .chat-upload {
    float: right;
  }

  .chat-box {
    .chat-content {
      height: 366px;
      overflow-y: auto;
      padding-top: 25px !important;
      display: flex;
      flex-wrap: wrap;

      .chat-item {
        display: inline-block;
        width: 100%;
        margin-bottom: 25px;

        .chat-details {
          img {
            width: 40px;
            border-radius: 50%;
            height: 40px;
          }

          margin-left: 10px;

          .chat-text {
            box-shadow: 0 4px 25px 0 rgba(0, 0, 0, 0.1);
            background-color: $white;
            padding: 10px 15px;
            border-radius: 3px;
            width: auto;
            display: inline-block;
            font-size: 12px;

            img {
              max-width: 100%;
              margin-bottom: 10px;
            }
          }

          .chat-time {
            margin-top: 5px;
            font-size: 12px;
            font-weight: 500;
            opacity: 0.6;
          }
        }
      }

      .chat-item.chat-right {
        .chat-details {
          margin-left: 0;
          margin-top: 70px;
          text-align: right;

          .chat-text {
            text-align: left;
            background-color: $default;
            color: $white;
          }
        }
      }

      .chat-item.chat-typing {
        .chat-details {
          .chat-text {
            height: 40px;
            width: 60px;
            background-position: center;
            background-size: 60%;
            background-repeat: no-repeat;
          }
        }
      }
    }

    .chat-form {
      padding: 0;
      position: relative;

      .form-control {
        border: none;
        padding: 15px;
        height: 50px;
        padding-right: 70px;
        font-size: 13px;
        font-weight: 500;
        box-shadow: none;
        outline: none;
      }

      .btn {
        padding: 0;
        width: 40px;
        height: 40px;
        border-radius: 50%;
        position: absolute;
        top: 50%;
        right: 0px;
        transform: translate(-50%, -50%);
        box-shadow: 0 4px 25px 0 rgba(0, 0, 0, 0.1);
        max-width: 50px;
        min-width: 38px;
        text-align: center;

        i {
          margin-left: 0;
        }
      }
    }
  }

  .main-chatsection {
    width: 100%;
  }
}

/*c-chat-comp end*/

