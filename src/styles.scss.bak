/*login page start*/
form mat-form-field:focus {
  border-color: $default;
  box-shadow: 0 0 0 2px $grey-one;
  caret-color: $default;
}

form .mat-button,
.mat-raised-button,
.mat-icon-button,
.mat-stroked-button,
.mat-flat-button,
.mat-fab,
.mat-mini-fab {
  font-size: 14px;
  font-weight: 500;
}

form .mat-button-ripple.mat-ripple,
.mat-button-focus-overlay {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  position: absolute;
  pointer-events: none;
  border-radius: inherit;
  outline: none;
}

*,
*::before,
*::after {
  box-sizing: border-box;
  border: 0;
  outline: 0;
  background: transparent;
