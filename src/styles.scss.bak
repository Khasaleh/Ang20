/* c-signup-form start*/
.c-signup-form{
  width: 700px;

  .box-head{
    &.title{
      display: flex;
      justify-content: center;
    }
  }
  .pathLine{
    background-color: $gray-500;
    height: 5px;
    width: 100%;
    position: absolute;
    z-index: 1;
  }
  .icon-container {
    box-sizing: border-box;
    background-color: $gray-700;
    width: 50px;
    height: 50px;
    margin: auto;
    border-radius: 50%;
    position: relative;
    padding: 5px;
    z-index: 10;
    &.detail-filled {
      background-color: $primary;
      & + p{
        color: $primary;
      }
      &.active{
        position: relative;
        &::after{
          content: '';
          position: absolute;
          bottom: -7px;
          left: 35%;
          border-left: 8px solid transparent;
          border-right: 8px solid transparent;
          border-top: 8px solid $primary;
          display: block;
        }
      }
    }
    & + p{
      border: 1px solid $black-one;
      text-align: center;
      padding: 0.25rem;
      margin-top: 0.5rem;
    }
  }
  .c-signup-business-type{
    .bus-type-cont{
      border: 1px solid $gray-500;
      border-radius: 0.5rem;
      .imgCont{
        margin-top: 0.25rem;
        text-align: center;
        img{
          width: 60px;
          height: 60px;
        }
      }
      &.active{
        border: 2px solid $primary;
      }
      .text-cont{
        h4{
          font-size: 16px;
        }
        p{
          font-size: 14px;
          position: relative;
          mat-icon{
            font-size: 14px;
            width: 14px;
            height: 14px;
            position: absolute;
            right: 5px;
            bottom: 0;
          }
        }
      }
    }
  }
  @media (max-width: 768px) {
    max-width: 100%;
    width: 100%;
  }
  @media (max-width: 600px) {
    width: auto;
    .box-head {
      flex-direction: column-reverse;
      .boxhead-left{
        text-align: center;
      }
    }
    .box-content{
      overflow-y: auto;
      height: 70vh
    }
    .pathLine{
      display: none;
    }
  }
}
/* c-signup-form end*/

