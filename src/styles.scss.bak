.h3,
h3 {
  font-size: 1.75rem;
}

label,
span,
p {
  font-weight: normal;
  margin: 0;
  line-height: normal;
  cursor: pointer;

}

label.iconm,
span.iconm,
p.iconm.iconm,
h1.iconm,
h2.iconm,
h3.iconm,
h4.iconm,
h5.iconm,
