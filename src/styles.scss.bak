/*header footer css start*/
.container {
  padding-left: 70px;
  padding-right: 70px;
  max-width: 100%;
}

.header-wrap a img {
  max-width: 140px;
  width: 100%;
  margin: 0px 5px 0px 0px;
}

.header-wrap a.logotxt {
  font-size: 40px;
  color: $default !important;
  display: flex;
  line-height: normal;
  text-decoration: none;
  align-items: center;
  letter-spacing: 0.4px;
}

.header-wrap,
.footer-static {
  display: flex;
  width: 100%;
  max-width: 100%;
  justify-content: space-between;
  padding: 5px 13px 10px;
  background: $white;
  overflow: initial;
}

.header-wrap .links,
.footer-static .links {
  padding: 0;
  margin: 0;
  list-style: none;
  display: flex;
  align-items: center;
}

.header-wrap .links ul,
.footer-static .links ul {
  padding: 0;
  margin: 0;
  list-style: none;
  display: inline-flex;
}

.header-wrap .links ul li {
  font-size: small;
  padding: 0px 2px 0px 0px;
  align-items: center;
  display: flex;
}
.header-wrap .links ul li button.username {
  border-radius: 30px;
  padding: 0;
  min-width: 28px;
}
.header-wrap .links ul li button.username img {
  width: 24px;
  border-radius: 30px;
}
.footer-static .links ul li {
  font-size: small;
  padding: 0px 15px 0px 0px;
  align-items: center;
  display: flex;
}

.header-wrap a.icon {
  display: none;
}

.header-wrap .links ul li a {
  font-size: 18px;
  font-weight: 500;
  font-family: "Myriad Pro";
  letter-spacing: 0.2px;
  min-width: 40px;
  line-height: 36px;
  padding: 0 10px;
}

.header-wrap .links ul li a span.mat-button-wrapper {
  color: $default;
}

.footer-static .links ul li a {
  font-size: 15px;
  font-weight: 500;
  font-family: "Myriad Pro";
  letter-spacing: 0.2px;
}

.footer-static {
  padding: 6px 15px 10px;
}

.footer-static .links.links.sociallinks a {
  align-items: center;
  display: flex;
  color: $default !important;
}

.field-icon {
  float: right;
  margin-top: -31px;
  position: relative;
  z-index: 2;
  right: 13px;
}

.nowrap {
  flex-wrap: nowrap !important;
}

.header,
footer {
  background: $white;
}

.center-content {
  height: 100%;
  max-width: 1920px;
  margin: 0 auto;
  display: flex;
  width: 100%;
  justify-content: space-between;
}

.header-wrap {
  max-width: 1920px !important;
  margin: 0 auto;
}

.header-wrap .links ul .mat-form-field-wrapper {
  padding: 0px !important;
}

.mat-select-panel {
  position: relative;
  top: 38px;
}

